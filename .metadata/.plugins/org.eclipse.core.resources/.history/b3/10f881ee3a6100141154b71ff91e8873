package com.DR.dLib;

import com.badlogic.gdx.graphics.Color;

public class CircleFillAnimation extends dAnimation {

	private Color circleColor;
	
	public CircleFillAnimation(dObject objectToAnimate, float duration, AnimationStatusListener listener, Color c) {
		super(objectToAnimate, duration, listener);
		circleColor = c;
	}

	@Override
	protected void animate(float time, float duration, float delta) {
		
	}

}
